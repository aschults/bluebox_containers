{
   "kind":"Service",
   "apiVersion":"v1",
   "metadata":{
      "name":"bhdns",
      "labels":{
         "app":"bhdns"
      }
   },
   "spec":{
      "ports": [
         {
           "port":53,
           "targetPort":"bhdns",
           "protocol":"UDP",
           "name":"bhdns"
         },
         {
           "port":53,
           "targetPort":"bhdns-tcp",
           "protocol":"TCP",
           "name":"bhdns-tcp"
         }
      ],
      "selector":{
         "app":"bhdns"
      },
      "type": "ClusterIP",
      "clusterIP": "10.0.0.12",
      "externalIPs" : [
          "192.168.2.254"
      ]

   }
}
