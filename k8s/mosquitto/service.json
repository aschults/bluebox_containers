{
   "kind":"Service",
   "apiVersion":"v1",
   "metadata":{
      "name":"mqtt",
      "labels":{
         "app":"mqtt",
         "mode":"standard"
      }
   },
   "spec":{
      "sessionAffinity":"ClientIP",
      "ports": [
         {
           "name":"mqtt",
           "port":1883,
           "targetPort":"mqtt"
         }
      ],
      "selector":{
         "app":"mqtt"
      },
      "type": "LoadBalancer",
      "externalIPs" : [
          "192.168.2.254"
      ]

   }
}
